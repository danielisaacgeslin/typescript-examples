// Place your settings in this file to overwrite default and user settings.
{
    "explorer.openEditors.visible": 0,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "app/**/*.js": true,
        "app/**/*.map": true,
        "test/**/*.js": true,
        "test/**/*.map": true,
        "**/*.js": true,
        "**/*.js.map": true,
        "prepend.js": false,
        "node_modules/**/*.js": false,
        "**/webpack.config.js": false
    },
    "typescript.tsdk": "node_modules/typescript/lib"
}